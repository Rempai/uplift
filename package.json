    "openapi-typescript-codegen": "^0.23.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prettier-plugin-svelte": "^2.9.0",
    "svelte": "^3.54.0",
    "svelte-highlight": "^7.1.2",
